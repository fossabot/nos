[binaries]
c = 'clang-16'
cpp = 'clang++-16'
c_ld = 'lld-16'
cpp_ld = 'lld-16'
ar = 'llvm-ar-16'
strip = 'llvm-strip-16'
pkg-config = 'pkg-config'
cmake = 'cmake'

[built-in options]
c_args = ['-target', 'x86_64-pc-elf']
cpp_args = ['-target', 'x86_64-pc-elf']

[host_machine]
system = 'pc'
cpu_family = 'x86_64'
cpu = 'x86_64'
endian = 'little'